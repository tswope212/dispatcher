<%= form_for(@team_admin) do |f| %>
  <% if @team_admin.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@team_admin.errors.count, "error") %> prohibited this team_admin from being saved:</h2>

      <ul>
      <% @team_admin.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.email_field :email %>
    <%= f.label :email %>
  </div>
  <div class="field">
    <%= f.text_field :first_name %>
    <%= f.label :first_name %>
  </div>
  <div class="field">
    <%= f.text_field :last_name %>
    <%= f.label :last_name %>
  </div>
  <div class="field">
    <%= f.text_field :phone_number %>
    <%= f.label :phone_number %>
  </div>
  <div class="field">
    <%= f.date_select :date_of_birth, :start_year => 1900, :discard_month => true, :discard_day => true %>
    <%= f.label :date_of_birth %>
  </div>
  <div class="field">
    <%= f.text_area :personal_statement %>
    <%= f.label :personal_statement, :size => '30x3' %>
  </div>
  <div class="field">
    <%= f.file_field :image %>
    <%= f.label :image %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

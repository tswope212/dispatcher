<%= form_for(@dispatch) do |f| %>
  <% if @dispatch.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@dispatch.errors.count, "error") %> prohibited this dispatch from being saved:</h2>

      <ul>
      <% @dispatch.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :team_id %><br />
    <%= f.select :team_id, options_from_collection_for_select(Team.all, :id, :name, @dispatch.team_id) %>
  </div>
  <div class="field">
    <%= f.label :job_id %><br />
    <%= f.select :job_id, options_from_collection_for_select(Job.all, :id, :name, @dispatch.job_id) %>
  </div>
  <div class="actions">
    <%= f.submit 'Dispatch Team' %>
  </div>
<% end %>
